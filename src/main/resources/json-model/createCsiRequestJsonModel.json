    },
    "minimalBalance": {
      "type": "number"
    },
    "thresholdAmount": {
      "type": "number"
    },
    "accountingDetails": {
      "type": "string"
    },
    "rateType": {
      "type": "string"
    },
    "taxPercentageRate": {
