      "type": "integer"
    },
    "productType": {
      "type": "string"
    },
    "productCode": {
      "type": "string"
    },
    "registerType": {
      "type": "string"
    },
    "mdmCode": {
      "type": "string"
    },
    "contractNumber": {
      "type": "string"
    },
    "contractDate": {
      "type": "string"
    },
    "priority": {
      "type": "integer"
    },
    "interestRatePenalty": {
